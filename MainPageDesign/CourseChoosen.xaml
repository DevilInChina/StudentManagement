<UserControl x:Class="StudentManagement.MainPageDesign.CourseChoosen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StudentManagement.MainPageDesign"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="900" Width="1200">

        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Menu x:Name="Menu1" Grid.ColumnSpan="4"  Background="#FF144BC3" VerticalAlignment="Stretch">
            <MenuItem Header="本学期课表" Click="CurJump"/>
            <MenuItem Header="历学年课表" Click="PrevJump"/>
        </Menu>
        <StackPanel Name="SubPanel" Grid.ColumnSpan="4" Margin="10,10,10,10" Grid.Row="1" />

    </Grid>
</UserControl>
