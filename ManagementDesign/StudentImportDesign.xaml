<UserControl x:Class="StudentManagement.ManagementDesign.StudentImportDesign"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StudentManagement.ManagementDesign"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="1200">
    <Grid>
        <Button x:Name="button_importStudent" Content="选取学生信息文件" HorizontalAlignment="Left" Margin="88,74,0,0" VerticalAlignment="Top" Width="151" Height="82" RenderTransformOrigin="-0.222,0.393" Click="button_importStudent_Click"/>
        <ProgressBar x:Name="Predeal" DataContext="预处理" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="946" Margin="244,74,0,0" Visibility="Hidden" ValueChanged="Predeal_ValueChanged"/>
        <ProgressBar x:Name="AddInfo" DataContext="添加到数据库" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="946" Margin="244,131,0,0" Visibility="Hidden"/>

    </Grid>
</UserControl>
